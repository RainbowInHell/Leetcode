function reverseNumber(num) {
    let reversed = 0;
    while (num > 0) {
        reversed = reversed * 10 + num % 10;
        num = Math.floor(num / 10);
    }

    return reversed;
}

/**
 * @param {number[]} nums
 * @return {number}
 */
const countDistinctIntegers = function(nums) {
    const distinctNums = new Set(nums);

    for (let num of nums) {
        const reversed = reverseNumber(num);
        distinctNums.add(reversed);
    }

    return distinctNums.size;
}

console.log(countDistinctIntegers([1,13,10,12,31]));